{
  "name": "water-client",
  "version": "0.1.0",
  "repository": "https://github.com/maathh/water-client",
  "scripts": {
    "build:app": "next build src/app",
    "build:functions": "cpx \"src/functions/**/*.*\" dist/functions",
    "build:public": "cpx \"src/app/static/**/*.*\" dist/public/static",
    "clean": "rimraf dist",
    "copy:deps": "cpx \"*{package.json,package-lock.json}\" dist/functions",
    "dev": "next src/app"
  },
  "dependencies": {
    "firebase-admin": "7.2.0",
    "firebase-functions": "2.2.1",
    "next": "8.1.0",
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "engines": {
    "node": "8"
  },
  "devDependencies": {
    "cpx": "1.5.0",
    "firebase-tools": "6.10.0",
    "rimraf": "2.7.1"
  }
}
